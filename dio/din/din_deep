#!/bin/bash

IOX_PATH="/home/unmesh/iox_test/"
source ${IOX_PATH}"common"
LOGGER_PATH=${IOX_PATH}"dio/din/logger"

err_parse_args="Error in parsing json object"

din_deep_test1()
{
	subscribe_logger_event &
	mosquitto_pub -m '30' -t "glp/0/17q2d9v/rq/dev/iox/dio/if/dia/1/di-val/monitor/rate" 
	parse_logger "${err_parse_args}"
}

din_deep_test2()
{
	subscribe_logger_event &
	mosquitto_pub -m '30' -t "glp/0/17q2d9v/rq/dev/iox/dio/if/di/1/di-vala/monitor/rate" 
	parse_logger
}

din_deep_test3()
{
	subscribe_logger_event &
#	mosquitto_sub -t "glp/0/17q2d9v/fb/dev/iox/dio/if/di/1" > ${LOGGER_PATH} &
	mosquitto_pub -m '{"di-val":{"value":{"type":"frequency","di-level":{"report-dur":1000,"detect-level":0,"pullup-cfg":true},"di-pulse":{"report-dur":10,"detect-level":0,"pullup-cfg":true},"di-frequency":{"report-dur":100,"detect-level":0,"pullup-cfg":true}},"monitor":{"rate":15,"cat":"data","inFeedback":true,"report":"change","throttle":0,"threshold":10}}}' -t "glp/0/17q2d9v/rq/dev/iox/dio/if/di/1"
	parse_logger
}

din_deep_test4()
{
	subscribe_logger_event &
#	mosquitto_sub -t "glp/0/17q2d9v/fb/dev/iox/dio/if/di/1" > ${LOGGER_PATH}  &
	mosquitto_pub -m '{"value":{"type":"level","di-level":{"report-dur":1000,"detect-level":0,"pullup-cfg":true},"di-pulse":{"report-dur":10,"detect-level":0,"pullup-cfg":true},"di-frequency":{"report-dur":100,"detect-level":0,"pullup-cfg":true}},"monitor":{"rate":100,"cat":"data","inFeedback":true,"report":"change","throttle":0,"threshold":10}}' -t "glp/0/17q2d9v/rq/dev/iox/dio/if/di/1/di-val"
	parse_logger
}

din_deep_test5()
{
	subscribe_logger_event &
#	mosquitto_sub -t "glp/0/17q2d9v/fb/dev/iox/dio/if/di/1" > ${LOGGER_PATH}  &
	mosquitto_pub -m '{"rate":50}' -t "glp/0/17q2d9v/rq/dev/iox/dio/if/di/1/di-val/monitor"
	parse_logger
}

din_deep_test6()
{
	subscribe_logger_event &
#	mosquitto_sub -t "glp/0/17q2d9v/fb/dev/iox/dio/if/di/1" > ${LOGGER_PATH}  &
	mosquitto_pub -m '30' -t "glp/0/17q2d9v/rq/dev/iox/dio/if/di/1/di-val/monitor/rate"
	parse_logger
}

din_deep_test1
din_deep_test2
din_deep_test3
din_deep_test4
din_deep_test5
din_deep_test6

kill_process

