dio_create_test6()
{
	start_time=$(date | awk '{print $4}')
 	subscribe_logger_event &
	pids+=($!)
	mosquitto_pub -m '{"action":"create","args":{"type":"900001050304DD00","provision":true}}' -t "glp/0/${SID}/rq/dev/${IOX_PROTOCOL}/${LOGICAL_ID}dio/do"
	parse_logger "${no_unid}"
	result "logger"
	pub=("mosquitto_pub -m '{"action":"create","args":{"type":"900001050304DD00","provision":true}}' -t '"glp/0/${SID}/rq/dev/${IOX_PROTOCOL}/${LOGICAL_ID}dio/do"'")
	result_logs "dio_create_test" ${LOG_DIO_CREATE}  ${start_time} "${pub}"
}

dio_create_test7()
{
	start_time=$(date | awk '{print $4}')
	subscribe_sts_event &
	pids+=($!)
  	subscribe_logger_event &
	pids+=($!)
	mosquitto_pub -m '{"action":"create","args":{"type":"900001050304DD00","provision":false,"unid":"256"}}' -t "glp/0/${SID}/rq/dev/${IOX_PROTOCOL}/${LOGICAL_ID}dio/do"
	parse_sts "${create_expected_state}"
	parse_logger "${no_unid}"
	result "sts"
	pub=("mosquitto_pub -m '{"action":"create","args":{"type":"900001050304DD00","provision":false,"unid":"256"}}' -t '"glp/0/${SID}/rq/dev/${IOX_PROTOCOL}/${LOGICAL_ID}dio/do"'")
	result_logs "dio_create_test" ${LOG_DIO_CREATE}  ${start_time} "${pub}"
}

