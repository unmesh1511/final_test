	execution_time ${start_time} ${end_time}
	echo "" | tee -a ${RESULT_LOG} ${DIO_PROVISION_LOG}
	echo "==============================================================================" | tee -a ${DIO_PROVISION_LOG}
	
}

setup_time

dio_provision_test1
dio_provision_test2
dio_provision_test3
dio_provision_test4
dio_provision_test5
dio_provision_test6

kill_process
