#!/bin/bash

IOX_PATH="/home/unmesh/iox_test/"
source ${IOX_PATH}"common"
LOGGER_PATH=${IOX_PATH}"dio/logger"
STS_PATH=${IOX_PATH}"dio/sts"
DIO_REPAIR_LOG=${IOX_PATH}"dio/repair_log"

err_repair_hndl="ERROR in iox_repair handle"

test_num=1

dio_repair_test1()
{
	echo -n "dio_repair_test"${test_num} | tee -a ${RESULT_LOG} ${DIO_REPAIR_LOG}
	((test_num++)) 
	start_time=$(date | awk '{print $4}')
	echo "" >> ${DIO_REPAIR_LOG}
	echo "START_TIME : "${start_time} >> ${DIO_REPAIR_LOG}
	subscribe_logger_event &
	mosquitto_pub -m '{"action":"repair"}' -t "glp/0/17q2d9v/rq/dev/iox/dio/do"
	echo "mosquitto_pub -m '{"action":"repair"}' -t '"glp/0/17q2d9v/rq/dev/iox/dio/do"'" >> ${DIO_LOAD_LOG}
	parse_logger "${err_repair_hndl}" ${DIO_LOAD_LOG}
	result "logger"
	end_time=$(date | awk '{print $4}')
	echo "END_TIME : "${end_time} >> ${DIO_REPAIR_LOG}
	execution_time ${start_time} ${end_time}
	echo "" | tee -a ${RESULT_LOG} ${DIO_REPAIR_LOG}
	echo "==============================================================================" | tee -a ${DIO_REPAIR_LOG}
}

setup_time

dio_repair_test1

kill_process
