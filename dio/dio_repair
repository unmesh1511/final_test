#!/bin/bash

source env_var.sh
source ${IOX_PATH}"/common.sh"
source ${IOX_PATH}"/iox.config"
source ${IOX_PATH}"/errors.sh"

LOG_EVENT_LOGGER=${IOX_PATH}"/dio/logger"
LOG_DIO_REPAIR=${IOX_PATH}"/dio/dio_repair_log"


dio_repair_test1()
{
	start_time=$(date | awk '{print $4}')
	subscribe_logger_event &
	pids+=($!)
	mosquitto_pub -m '{"action":"repair"}' -t "glp/0/${SID}/rq/dev/iox/${DIO_DEV_NAME}/do"
	parse_logger "${err_repair_hndl}"
	result "logger"
	pub=("mosquitto_pub -m '{"action":"repair"}' -t '"glp/0/${SID}/rq/dev/iox/${DIO_DEV_NAME}/do"'")
	result_logs "dio_repair_test" ${LOG_DIO_REPAIR}  ${start_time} "${pub}"
}

setup_time

dio_repair_test1

kill_process
